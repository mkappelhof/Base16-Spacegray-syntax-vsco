{
	"name": "Base16 Spacegray",
	"type": "dark",
	"colors": {
		"editor.background": "#222222",
		"editor.foreground": "#c0c5ce",
		"activityBarBadge.background": "#bf616a",
		"sideBarTitle.foreground": "#99cc99"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#65737e"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#bf616a"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#c0c5ce"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword", 
			"settings": {
				"foreground": "#c0c5ce"
			}
    },
    {
      "name": "Existential operator",
      "scope": "keyword.operator.existential",
      "settings": {
				"foreground": "#8fa1b3"
			}
    },
    {
			"name": "Storage",
			"scope": [
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#b48ead"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.directive",
        "punctuation.definition.keyword", 
				"punctuation.section.embedded",
				"keyword.other.template",
        "keyword.other.substitution"
			],
			"settings": {
				"foreground": "#b48ead"
			}
    },
    {
      "name": "Punctuation", 
      "scope": "punctuation", 
      "settings": {
        "foreground": "#c0c5ce"
      }
    }, 
    {
      "name": "Punctuation embedded", 
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#ab7967"
      }
    },
    {
      "name": "Destructuring brackets", 
      "scope": [
        "punctuation.definition.destructuring.begin.bracket.square", 
        "punctuation.definition.destructuring.end.bracket.square", 
        "meta.variable.assignment.destructured.array", 
      ],
      "settings": {
        "foreground": "#bf616a"
      }
    },
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#bf616a"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#8fa1b3"
			}
    },
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#c0c5ce"
			}
    },
    {
      "name": "Variable other", 
      "scope": "variable.other.property",
      "settings": {
				"foreground": "#c0c5ce"
			}
    }, 
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#c0c5ce"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#d08770"
			}
    },
    {
      "name": "Keyword Package, Import", 
      "scope": [
        "keyword.other.package",
        "keyword.other.import"
      ],
      "settings": {
				"foreground": "#b48ead"
			}
    },
    {
      "name": "Storage modifier Package, Import", 
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import"
      ],
      "settings": {
				"foreground": "#8fa1b3"
			}
    },
    {
      "name": "Variable parameter",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c0c5ce"
      }
    },
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#a3be8c"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#ebcb8b"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#c0c5ce"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#c0c5ce"
			}
    },
    {
      "name": "CSS Punctuation definition",
